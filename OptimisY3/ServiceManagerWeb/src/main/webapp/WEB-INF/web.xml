<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>Service Manager Web</display-name>
  <!-- Servlets -->
   <servlet>
    <servlet-name>guiserviceServlet</servlet-name>
    <servlet-class>eu.optimis.sm.gui.server.ServiceManagerWebServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>guiserviceServlet</servlet-name>
    <url-pattern>/servicemanagerweb/guiservice</url-pattern>
  </servlet-mapping>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>ServiceManagerWeb.html</welcome-file>
  </welcome-file-list>

 <!-- security-constraint>
