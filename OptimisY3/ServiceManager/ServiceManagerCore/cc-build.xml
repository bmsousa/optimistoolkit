<?xml version="1.0"?>
<project name="ServiceManager" default="update-src" basedir=".">
	<property name="base.dir" value="." />
	<target name="update-src">
		<echo>Updating source from Subversion</echo>
		<exec executable="svn" dir="${base.dir}">
			<arg line="up" />
		</exec>
	</target>
</project>

